.contenedormenu
	background-image url('../assets/home/partido.jpg')
	color white
	display inline-flex
	flex-direction column
	height 700px
	width 100%
.menuquiniela
	display flex
	justify-content center
	height 50px
	width 100%
	div
		align-items space-between
		border 1px solid greenEucalyp
		bordes 7px
		display inline-flex
		flex-direction row
		justify-content space-around
		height 50px
		width 50%
		a
			transition ease 0.5s
			-webkit-transition ease 0.5s
			align-items center
			color white
			display inline-flex
			font-size 12px
			font-weight bold
			justify-content center
			text-decoration none
		a:hover
			transition ease 0.5s
			-webkit-transition ease 0.5s
			align-items center
			color greenEucalyp
			display inline-flex
			text-decoration nones
